{
  "name": "Github-Tate-Young",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Tate-Young/Tate-Young.github.io.git",
  "author": "Tate-Young <smd_tate@163.com>",
  "port": "2333",
  "scripts": {
    "start": "#jekyll serve\n yarn cowsay && cross-var bundle exec jekyll serve --port $npm_package_port",
    "test": "cross-var echo $npm_package_version",
    "commit": "scripty $*",
    "cowsay": "cross-var echo $npm_package_name@$npm_package_version | cowsay | lolcatjs"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "cross-var": "^1.1.0",
    "editorconfig": "^0.15.0",
    "husky": "^1.3.1",
    "imagemin-cli": "^3.0.0",
    "markdownlint": "^0.11.0",
    "scripty": "^1.7.2"
  },
  "dependencies": {
    "npm": "^6.13.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "say -v Mei-Jia '提交完毕' && echo 'tate & snow' | cowsay | lolcatjs"
    }
  }
}
